{
	"$schema": "https://unpkg.com/knip@latest/schema.json",
	"ignoreExportsUsedInFile": { "interface": true, "type": true },
	"ignoreWorkspaces": ["examples/sse"],
	"workspaces": {
		".": {
			"entry": ["src/index.ts!"],
			"project": ["src/**/*.ts!"]
		},
		"examples/*": {
			"entry": ["src/{server,client,index}.ts!"]
		}
	}
}
